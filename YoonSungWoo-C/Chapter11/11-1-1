#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

//배열 반복 입력
//입력된 정수 중 최댓값
//입력된 정수의 총 합

int main(void)
{
	int arr[5];
	int max = 0; 
	int min = 0;
	int	sum = 0 ;
	for (int i = 0; i < 5; i++) {
		scanf("%d", &arr[i]);
		if (max < arr[i]) {
			max = arr[i];
		}
		if (i == 0 || min > arr[i]) {
			min = arr[i];
		}
		
		sum += arr[i];
	}
		printf("최댓값 : %d 최솟값 : %d 합 : %d", max, min, sum);
	
}
